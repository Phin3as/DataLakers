<?xml version="1.0" encoding="UTF-8"?>
 <!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.2//EN" "http://java.sun.com/j2ee/dtds/web-app_2_2.dtd">
 <web-app>
     <display-name>DataLakers</display-name>
     
     <servlet>
         <servlet-name>MasterServlet</servlet-name>
         <servlet-class>edu.upenn.cis550.servlet.MasterServlet</servlet-class>
     </servlet>
	 
     <servlet>
         <servlet-name>SearchServlet</servlet-name>
         <servlet-class>edu.upenn.cis550.servlet.SearchServlet</servlet-class>
         <load-on-startup>1</load-on-startup>
     </servlet>
     
     <servlet>
         <servlet-name>UploadServlet</servlet-name>
         <servlet-class>edu.upenn.cis550.servlet.UploadServlet</servlet-class>
         <load-on-startup>1</load-on-startup>
     </servlet>     

     <servlet>
         <servlet-name>PermissionServlet</servlet-name>
         <servlet-class>edu.upenn.cis550.servlet.PermissionServlet</servlet-class>
         <load-on-startup>1</load-on-startup>
     </servlet> 

     <servlet>
         <servlet-name>ListDocServlet</servlet-name>
         <servlet-class>edu.upenn.cis550.servlet.ListDocServlet</servlet-class>
         <load-on-startup>1</load-on-startup>
     </servlet> 
     	 
	 <servlet-mapping>
         <servlet-name>MasterServlet</servlet-name>
         <url-pattern>/*</url-pattern>
     </servlet-mapping>

	 <servlet-mapping>
         <servlet-name>SearchServlet</servlet-name>
         <url-pattern>/search</url-pattern>
     </servlet-mapping>
     
	 <servlet-mapping>
         <servlet-name>UploadServlet</servlet-name>
         <url-pattern>/upload</url-pattern>
     </servlet-mapping>
     
	 <servlet-mapping>
         <servlet-name>PermissionServlet</servlet-name>
         <url-pattern>/assign</url-pattern>
     </servlet-mapping>

	 <servlet-mapping>
         <servlet-name>ListDocServlet</servlet-name>
         <url-pattern>/listDoc</url-pattern>
     </servlet-mapping>
</web-app>